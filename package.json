{
  "name": "vue-ele-component-ff",
  "version": "2.1.17",
  "private": false,
  "description": "封装了ff常用的组件库,如日期选择器、表格及弹窗等",
  "scripts": {
    "build": "npm run build:npm & npm run build:dist & npm run build:storybook",
    "build:npm": "vue-cli-service build",
    "build:dist": "cross-env NODE_ENV=productiondist env_config=proddist vue-cli-service build",
    "build:storybook": "build-storybook",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service serve",
    "lib": "vue-cli-service build --target lib --name vue-ele-component-ff --dest lib/all src/components/common/index.js",
    "lib:part": "vue-cli-service build --target lib --name dialog-with-btn   --dest lib/part src/components/common/DialogWithBtn/index.js",
    "storybook": "start-storybook -p 6006",
    "open": "node bin/openBrowser",
    "prepublishOnly": "standard-version"
  },
  "main": "lib/index/index.js",
  "files": [
    "lib",
    "src/components/common"
  ],
  "dependencies": {
    "cross-env": "~7.0.3",
    "echarts": "~4.1.0",
    "js-cookie": "^2.2.1",
    "sql-formatter": "^4.0.2",
    "vue-clipboard2": "^0.3.3",
    "vue-codemirror": "^4.0.6"
  },
  "devDependencies": {
    "@babel/core": "~7.12.13",
    "@storybook/addon-actions": "~6.2.8",
    "@storybook/addon-essentials": "~6.2.8",
    "@storybook/addon-links": "~6.2.8",
    "@storybook/addon-storysource": "~6.2.8",
    "@storybook/vue": "~6.2.8",
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-unit-jest": "^4.5.12",
    "@vue/cli-service": "~4.4.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "~8.2.2",
    "babel-plugin-import": "~1.13.3",
    "core-js": "^3.6.5",
    "element-ui": "~2.3.9",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "~4.2.5",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.2",
    "screenfull": "4.2.0",
    "standard-version": "~9.3.1",
    "svg-sprite-loader": "4.1.3",
    "svgo": "1.2.0",
    "vue": "^2.6.11",
    "vue-router": "~3.3.4",
    "vue-template-compiler": "^2.6.11",
    "vuex": "3.1.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1% in CN",
    "last 2 versions",
    "not ie <= 8",
    "Android >= 4.0",
    "and_qq 1.2",
    "Safari >= 6",
    "and_chr >=38"
  ],
  "husky": {
    "hooks": {
      "pre-push": ""
    }
  },
  "keywords": [
    "vue",
    "element-ui",
    "dialog",
    "datePicker",
    "table"
  ],
  "license": "MIT",
  "repository": {
    "type": "git"
  }
}
